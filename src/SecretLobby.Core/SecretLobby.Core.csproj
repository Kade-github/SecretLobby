<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
  	<AssemblyName>SecretLobby.Core</AssemblyName>
  	<RootNamespace>SecretLobby</RootNamespace>
    <TargetFrameworks Condition=" '$(OS)' == 'Windows_NT' ">net461;net471;net472;netstandard2.0;netstandard2.1</TargetFrameworks>
    <TargetFrameworks Condition=" '$(OS)' != 'Windows_NT' ">netstandard2.0;netstandard2.1</TargetFrameworks>
    <Version>1.0.1</Version>
    <OutputPath>..\..\Builds\$(MSBuildProjectName)\$(Configuration)</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
</Project>
